<h3> List of Students </h3>
<table>
  <thead>
    <tr>
      <th> First Name </th>
      <th> Last Name </th>
      <th>  Email </th>
      <th colspan="3">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td> <%= student.first_name %> </td>
        <td> <%= student.last_name %> </td>
        <td> <%= student.email %> </td>
        <td> <%= link_to 'View', student%> </td>
      </tr>
    <% end %> 
  </tbody>
</table>